{
  "family": "frontend",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::753591596815:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "frontend",
      "image": "REPLACE_IMAGE",
      "essential": true,
      "portMappings": [
        { "containerPort": 3000, "protocol": "tcp" }
      ],
      "environment": [
        {
          "name": "VITE_API_URL",
          "value": "http://api-tg-1598256574.eu-north-1.elb.amazonaws.com:8000"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group":         "/ecs/frontend",
          "awslogs-region":        "eu-north-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
